{
  "name": "mobile-ui-elements",
  "version": "0.0.0",
  "dependencies": {
    "platform": "Polymer/platform"
  },
  "devDependencies": {
    "modernizr": "~2.6.2",
    "polymer": "Polymer/polymer",
    "polymer-elements": "Polymer/polymer-elements",
    "polymer-ui-elements":  "Polymer/polymer-ui-elements",
    "jquery": "latest",
    "underscore": "latest",
    "backbone": "latest",
    "mobilesdk-shared": "forcedotcom/SalesforceMobileSDK-Shared#master",
    "ratchet": "latest"
  },
  "exportsOverride": {
    "modernizr": {
      ".": "modernizr*.js"
    },
    "jquery": {
      ".": ["dist/*"]
    },
    "backbone": {
      ".": "backbone*.js"
    },
    "underscore": {
      ".": "underscore*.js"
    },
    "mobilesdk-shared": {
      ".": [
        "libs/*js",
        "test/Mock*.js"
      ]
    },
    "polymer": {
      "Polymer/.": [
        "polymer.js",
        "polymer.html",
        "polymer-body.html"
      ]
    },
    "polymer-*": {
      "Polymer/.": ["."]
    },
    "ratchet": {
      ".": "dist/."
    },
    "web-animations-js": {
      "Polymer/.": "web-animations.js"
    }
  }
}
